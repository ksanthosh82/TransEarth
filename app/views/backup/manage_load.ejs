									<div class="col-lg-12">
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-map-marker fa-2"></i></span>
											<input type="text" class="form-control" name="owner_line2" id="owner_line2" ng-model="load.owner.address.line2"
												   ng-disabled="load.owner.details_same_as_user" ng-maxlength="35" maxlength="35"
												   placeholder="Enter Address Line2">
										</div>
										<i class="form-control-feedback"
										   ng-class="{ 'glyphicon glyphicon-remove' : loadForm.owner_line2.$invalid,
										'glyphicon glyphicon-ok' : !(loadForm.owner_line2.$invalid),
										'has-feedback' : loadForm.owner_line2.$valid}"
										   for="owner_line2">
										</i>
										<div ng-if="loadForm.owner_line2.$invalid"
											 class="help-block"
											 ng-class="{ 'has-error' : loadForm.owner_line2.$invalid,
										'has-success' : !(loadForm.owner_line2.$invalid),
										'has-feedback' : loadForm.owner_line2.$invalid}"
											 for="owner_line2">Line 2 is optional
										</div>
									</div>
								</div>

								<!-- CITY -->
								<div class="form-group-sm"
									 ng-class="{ 'has-error' : !load.owner.address.mapLocation.isSelected,
									'has-success' : load.owner.address.mapLocation.isSelected,
									'has-feedback' : load.owner.address.mapLocation.isSelected}">
									<!-- label class="col-lg-3 control-label">City</label -->
									<div class="col-lg-12">
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-map-marker fa-2"></i></span>
											<!-- input type="text" class="form-control" name="owner_city" id="owner_city" ng-model="load.owner.address.city"
												placeholder="Enter City" ng-required="true" -->
											<div id="city" ng-if="!load.owner.details_same_as_user">
												<google-places
														tagId="owner_city" tagName="owner_city" location=load.owner.address.mapLocation></google-places>
											</div>
											<div id="city" ng-if="load.owner.details_same_as_user">
												<input type="text" class="form-control" name="owner_city" id="owner_city"
													   ng-disabled="true" readonly
													   ng-model="load.owner.address.mapLocation.place" ng-required="true">
											</div>
										</div>
										<i class="form-control-feedback"
										   ng-class="{ 'glyphicon glyphicon-remove' : !load.owner.address.mapLocation.isSelected,
									'glyphicon glyphicon-ok' : load.owner.address.mapLocation.isSelected,
									'has-feedback' : load.owner.address.mapLocation.isSelected}"
										   for="owner_city">
										</i>
										<div ng-if="!load.owner.address.mapLocation.isSelected"
											 class="help-block"
											 ng-class="{ 'has-error' : !load.owner.address.mapLocation.isSelected,
									'has-success' : load.owner.address.mapLocation.isSelected,
									'has-feedback' : load.owner.address.mapLocation.isSelected}"
											 for="owner_city">City is mandatory
										</div>
									</div>
									<!-- pre>{{userForm.city.$invalid}}</pre>
									<pre>{{userForm.city.$error}}</pre -->
								</div>

								<!-- STATE -->
								<!-- div class="form-group-sm"
									 ng-class="{ 'has-error' : loadForm.owner_state.$invalid,
									'has-success' : !(loadForm.owner_state.$invalid),
									'has-feedback' : loadForm.owner_state.$invalid}">
									<div class="col-lg-12">
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-map-marker fa-2"></i></span>
											<input type="text" class="form-control" name="owner_state" id="owner_state" ng-model="load.owner.address.state" placeholder="Enter State" ng-minlength="1" ng-maxlength="35" ng-required="true">
										</div>
										<i class="form-control-feedback"
										   ng-class="{ 'glyphicon glyphicon-remove' : loadForm.owner_state.$invalid,
									'glyphicon glyphicon-ok' : !(loadForm.owner_state.$invalid),
									'has-feedback' : loadForm.owner_state.$valid}"
										   for="owner_state">
										</i>
										<div ng-if="loadForm.owner_state.$invalid"
											 class="help-block"
											 ng-class="{ 'has-error' : loadForm.owner_state.$invalid,
									'has-success' : !(loadForm.owner_state.$invalid),
									'has-feedback' : loadForm.owner_state.$invalid}"
											 for="owner_state">State is mandatory
										</div>
									</div>
								</div -->

								<!-- PINCODE -->
								<div class="form-group-sm"
									 ng-class="{ 'has-error' : loadForm.owner_pincode.$invalid,
									'has-success' : !(loadForm.owner_pincode.$invalid),
									'has-feedback' : loadForm.owner_pincode.$invalid}">
									<!-- label class="col-lg-3 control-label">PIN</label -->
									<div class="col-lg-12">
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-map-marker fa-2"></i></span>
											<input type="text" numbers-only class="form-control" name="owner_pincode" id="owner_pincode"
												   ng-model="load.owner.address.pincode" placeholder="Enter Pincode"
												   ng-disabled="load.owner.details_same_as_user"
												   ng-minlength="6" ng-maxlength="6" maxlength="6" ng-required="true">
										</div>
										<i class="form-control-feedback"
										   ng-class="{ 'glyphicon glyphicon-remove' : loadForm.owner_pincode.$invalid,
									'glyphicon glyphicon-ok' : !(loadForm.owner_pincode.$invalid),
									'has-feedback' : loadForm.owner_pincode.$valid}"
										   for="owner_pincode">
										</i>
										<div ng-if="loadForm.owner_pincode.$invalid"
											 class="help-block"
											 ng-class="{ 'has-error' : loadForm.owner_pincode.$invalid,
									'has-success' : !(loadForm.owner_pincode.$invalid),
									'has-feedback' : loadForm.owner_pincode.$invalid}"
											 for="owner_pincode">Pincode is mandatory
										</div>
									</div>
									<!-- pre>{{userForm.user_type.$valid}}</pre -->
								</div>

								<!-- CONTACTNO -->
								<div class="form-group-sm"
									 ng-class="{ 'has-error' : loadForm.owner_contact.$invalid,
									'has-success' : !(loadForm.owner_contact.$invalid),
									'has-feedback' : loadForm.owner_contact.$invalid}">
									<!-- label class="col-lg-3 control-label">Contact Number</label -->
									<div class="col-lg-12">
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-mobile fa-2"></i></span>
											<input type="text" numbers-only class="form-control" name="owner_contact" id="owner_contact"
												   ng-disabled="load.owner.details_same_as_user"
												   ng-model="load.owner.contact" placeholder="Enter Contact Number"
												   ng-minlength="10" ng-maxlength="11" maxlength="11" ng-required="true">
										</div>
										<i class="form-control-feedback"
										   ng-class="{ 'glyphicon glyphicon-remove' : loadForm.owner_contact.$invalid,
									'glyphicon glyphicon-ok' : !(loadForm.owner_contact.$invalid),
									'has-feedback' : loadForm.owner_contact.$valid}"
										   for="owner_contact">
										</i>
										<div ng-if="loadForm.owner_contact.$invalid"
											 class="help-block"
											 ng-class="{ 'has-error' : loadForm.owner_contact.$invalid,
									'has-success' : !(loadForm.owner_contact.$invalid),
									'has-feedback' : loadForm.owner_contact.$invalid}"
											 for="owner_contact">Contact Number is mandatory
										</div>
									</div>
									<!-- pre>{{userForm.user_type.$valid}}</pre -->
								</div>
							</fieldset>
						</div>
					</div>
					<button class="btn btn-success nextBtn btn-sm pull-right" type="button">Next</button>
				</div>
			</div>
		</div>
		<div class="row setup-content" id="step-2">
			<div class="col-xs-6 col-md-offset-3">
				<div class="col-md-12">
					<h3> Step 2</h3>
					<div class="panel panel-info">
						<div class="panel-heading">
							<h5>Company Details</h5>
						</div>
						<div class="panel-body">
							<fieldset>
								<!-- Indicator for user registered details carryover-->
								<div class="form-group-sm">
									<div class="col-md-offset-1 col-md-12">
										<div class="input-group">
											<input type="checkbox"
												   ng-change="canDisableCompanyDetails()"
												   ng-value="false" name="user_company1" id="user_company1" ng-model="load.company.details_same_as_user"/>
											<label for="user_company1">&nbsp;Carry over registered user details?</label>
											<!--pre>{{load.company.details_same_as_user}}</pre-->
										</div>
									</div>
								</div>
								<!-- Company NAME -->
								<div class="form-group-sm"
									 ng-class="{ 'has-error' : loadForm.company_name.$invalid,
							 				'has-success' : !(loadForm.company_name.$invalid),
					 						'has-feedback' : loadForm.company_name.$invalid}">
									<label class="col-lg-3 control-label">Company Name</label>
									<div class="col-lg-9">
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-user"></i></span>
											<input type="text" class="form-control" name="company_name" id="company_name" ng-model="load.company.name"
												   ng-disabled="load.company.details_same_as_user"
												   placeholder="Enter Company Name" ng-maxlength="35" maxlength="35" ng-required="true">
										</div>
										<i class="form-control-feedback"
										   ng-class="{ 'glyphicon glyphicon-remove' : loadForm.company_name.$invalid,
							 				'glyphicon glyphicon-ok' : !(loadForm.company_name.$invalid),
					 						'has-feedback' : loadForm.company_name.$valid}"
										   for="company_name">
										</i>
										<div ng-if="loadForm.company_name.$invalid"
											 class="help-block"
											 ng-class="{ 'has-error' : loadForm.company_name.$invalid,
							 				'has-success' : !(loadForm.company_name.$invalid),
					 						'has-feedback' : loadForm.company_name.$invalid}"
											 for="company_name">Company Name is mandatory
										</div>
									</div>
									<!-- pre>{{userForm.user_type.$valid}}</pre -->
								</div>

								<!-- Indicator for same address and mobile -->
								<div class="form-group-sm">
									<div class="col-md-offset-3 col-md-9">
										<div class="input-group">
											<input type="checkbox"
												   ng-change="canDisableSameAddress()"
												   ng-disabled="load.company.details_same_as_user"
												   ng-value="false" name="same_address" id="same_address" ng-model="load.company.address_same_as_owner"/>
											<label>&nbsp;Is Address same as Owner?</label>
											<!--pre>{{load.same_address}}</pre -->
										</div>
									</div>
								</div>
								<!-- Address -->
								<div class="form-group-sm">
									<label class="col-lg-12 strong">Address</label>
								</div>
								<!-- div class="form-group-sm">
									<label class="col-lg-3 control-label"></label>
									<div class="col-lg-9">
										<div class="input-group">
											<input type="checkbox" ng-click="" class="form-control" name="same_address" id="same_address" ng-model="load.same_address">
										</div>
									</div>
								</div -->

								<!-- Address Line 1 -->
								<div class="form-group-sm"
									 ng-class="{ 'has-error' : loadForm.company_line1.$invalid,
							 				'has-success' : !(loadForm.company_line1.$invalid),
					 						'has-feedback' : loadForm.company_line1.$invalid}">
									<!-- label class="col-lg-3 control-label">Address Line 1</label -->
									<div class="col-lg-12">
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-map-marker fa-2"></i></span>
											<input type="text" class="form-control" name="company_line1" id="company_line1"
												   ng-disabled="load.company.address_same_as_owner||load.company.details_same_as_user"
												   ng-model="load.company.address.line1" placeholder="Enter Address Line1"
												   ng-maxlength="35" maxlength="35" ng-required="true">
										</div>
										<i class="form-control-feedback"
										   ng-class="{ 'glyphicon glyphicon-remove' : loadForm.company_line1.$invalid,
							 				'glyphicon glyphicon-ok' : !(loadForm.company_line1.$invalid),
					 						'has-feedback' : loadForm.company_line1.$valid}"
										   for="company_line1">
										</i>
										<div ng-if="loadForm.company_line1.$invalid"
											 class="help-block"
											 ng-class="{ 'has-error' : loadForm.company_line1.$invalid,
							 				'has-success' : !(loadForm.company_line1.$invalid),
					 						'has-feedback' : loadForm.company_line1.$invalid}"
											 for="company_line1">Line 1 is mandatory and should be of minimum 10 characters
										</div>
									</div>
								</div>

								<!-- Address Line 2 -->
								<div class="form-group-sm"
									 ng-class="{ 'has-error' : loadForm.company_line2.$invalid,
							 				'has-success' : !(loadForm.company_line2.$invalid),
					 						'has-feedback' : loadForm.company_line2.$invalid}">
									<!-- label class="col-lg-3 control-label">Address Line 2</label -->
									<div class="col-lg-12">
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-map-marker fa-2"></i></span>
											<input type="text" class="form-control" name="company_line2" id="company_line2"
												   ng-disabled="load.company.address_same_as_owner||load.company.details_same_as_user"
												   ng-maxlength="35" maxlength="35"
												   ng-model="load.company.address.line2" placeholder="Enter Address Line2">
										</div>
