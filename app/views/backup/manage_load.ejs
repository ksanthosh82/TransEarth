			<div class="stepwizard-step">
				<a href="#step-2" type="button" class="btn btn-default btn-circle" disabled="disabled">2</a>
				<p>Step 2</p>
			</div>
			<div class="stepwizard-step">
				<a href="#step-3" type="button" class="btn btn-default btn-circle" disabled="disabled">3</a>
				<p>Step 3</p>
			</div>
			<div class="stepwizard-step">
				<a href="#step-4" type="button" class="btn btn-default btn-circle" disabled="disabled">3</a>
				<p>Step 4</p>
			</div>
			<div class="stepwizard-step">
				<a href="#step-5" type="button" class="btn btn-default btn-circle" disabled="disabled">3</a>
				<p>Step 5</p>
			</div>

		</div>
	</div>

	<form role="form" name="loadForm" id="loadForm" action="" method="post" ng-submit="submitForm()">
		<div class="row setup-content" id="step-1">
			<div class="col-xs-6 col-md-offset-3">
				<div class="col-md-12">
					<h3> Step 1</h3>
					<div class="panel panel-info">
						<div class="panel-heading">
							<h5>Owner Details</h5>
						</div>
						<div class="panel-body">
							<fieldset>
								<!-- Indicator for user registered details carryover-->
								<div class="form-group-sm">
									<div class="col-md-offset-1 col-md-12">
										<div class="input-group">
											<input type="checkbox"
												   ng-change="canDisableOwnerDetails()"
												   ng-value="false" name="user_owner" id="user_owner" ng-model="load.owner.details_same_as_user"/>
											<label>&nbsp;Carry over registered user details?</label>
											<!--pre>{{truck.same_address}}</pre -->
										</div>
									</div>
								</div>
								<!-- FIRST NAME -->
								<div class="form-group-sm"
									 ng-class="{ 'has-error' : loadForm.first_name.$invalid,
										'has-success' : !(loadForm.first_name.$invalid),
										'has-feedback' : loadForm.first_name.$invalid}">
									<label class="col-lg-3 control-label">First Name</label>
									<div class="col-lg-9">
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-user"></i></span>
											<input type="text" class="form-control" name="first_name" id="first_name" ng-model="load.owner.first_name"
												   ng-disabled="load.owner.details_same_as_user"
												   placeholder="Enter Owner First Name" ng-maxlength="35" maxlength="35" ng-required="true">
										</div>
										<i class="form-control-feedback"
										   ng-class="{ 'glyphicon glyphicon-remove' : loadForm.first_name.$invalid,
										'glyphicon glyphicon-ok' : !(loadForm.first_name.$invalid),
										'has-feedback' : true}"
										   for="first_name">
										</i>
										<div ng-if="loadForm.first_name.$invalid"
											 class="help-block"
											 ng-class="{ 'has-error' : loadForm.first_name.$invalid,
										'has-success' : !(loadForm.first_name.$invalid),
										'has-feedback' : loadForm.first_name.$invalid}"
											 for="first_name">First Name is mandatory
										</div>
									</div>
									<!-- pre>{{userForm.user_type.$valid}}</pre -->
								</div>
								<!-- LAST NAME -->
								<div class="form-group-sm"
									 ng-class="{ 'has-error' : loadForm.last_name.$invalid,
										'has-success' : !(loadForm.last_name.$invalid),
										'has-feedback' : loadForm.last_name.$invalid}">
									<label class="col-lg-3 control-label">Last Name</label>
									<div class="col-lg-9">
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-user"></i></span>
											<input type="text" class="form-control" name="last_name" id="last_name" ng-model="load.owner.last_name"
												   ng-disabled="load.owner.details_same_as_user"
												   placeholder="Enter Last Name" ng-maxlength="35" maxlength="35" ng-required="true">
										</div>
										<i class="form-control-feedback"
										   ng-class="{ 'glyphicon glyphicon-remove' : loadForm.last_name.$invalid,
										'glyphicon glyphicon-ok' : !(loadForm.last_name.$invalid),
										'has-feedback' : loadForm.last_name.$valid}"
										   for="last_name">
										</i>
										<div ng-if="loadForm.last_name.$invalid"
											 class="help-block"
											 ng-class="{ 'has-error' : loadForm.last_name.$invalid,
										'has-success' : !(loadForm.last_name.$invalid),
										'has-feedback' : loadForm.last_name.$invalid}"
											 for="last_name">Last Name is mandatory
										</div>
									</div>
									<!-- pre>{{userForm.user_type.$valid}}</pre -->
								</div>
								<!-- Address -->
								<div class="form-group-sm">
									<label class="col-lg-3 strong">Address</label>
								</div>
								<!-- Address Line 1 -->
								<div class="form-group-sm"
									 ng-class="{ 'has-error' : loadForm.owner_line1.$invalid,
										'has-success' : !(loadForm.owner_line1.$invalid),
										'has-feedback' : loadForm.owner_line1.$invalid}">
									<!-- label class="col-lg-3 control-label">Address Line 1</label -->
									<div class="col-lg-12">
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-map-marker fa-2"></i></span>
											<input type="text" class="form-control" name="owner_line1" id="owner_line1" ng-model="load.owner.address.line1"
												   ng-disabled="load.owner.details_same_as_user"
												   placeholder="Enter Address Line1" ng-maxlength="35" maxlength="35" ng-required="true">
										</div>
										<i class="form-control-feedback"
										   ng-class="{ 'glyphicon glyphicon-remove' : loadForm.owner_line1.$invalid,
										'glyphicon glyphicon-ok' : !(loadForm.owner_line1.$invalid),
										'has-feedback' : loadForm.owner_line1.$valid}"
										   for="owner_line1">
										</i>
										<div ng-if="loadForm.owner_line1.$invalid"
											 class="help-block"
											 ng-class="{ 'has-error' : loadForm.owner_line1.$invalid,
										'has-success' : !(loadForm.owner_line1.$invalid),
										'has-feedback' : loadForm.owner_line1.$invalid}"
											 for="owner_line1">Line 1 is mandatory
										</div>
									</div>
								</div>

								<!-- Address Line 2 -->
								<div class="form-group-sm"
									 ng-class="{ 'has-error' : loadForm.owner_line2.$invalid,
										'has-success' : !(loadForm.owner_line2.$invalid),
										'has-feedback' : loadForm.owner_line2.$invalid}">
